<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RestrictTelesalesCallTime</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
 PreferredCallTimeTelesales__c &lt; TIMEVALUE(&quot;7:30:00.000&quot;),
 PreferredCallTimeTelesales__c &gt; TIMEVALUE(&quot;23:30:00.000&quot;) )</errorConditionFormula>
    <errorMessage>Please select a time for Preferred Call Time that is between 7:30am - 11:30pm to update this record</errorMessage>
</ValidationRule>
