<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IsCurrentOwnerCurrentUser__c</fullName>
    <description>Formula field to check if the owner in custom owner field is current user for list view</description>
    <externalId>false</externalId>
    <formula>aforza__Owner__c ==  $User.Id</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Current Owner Current User</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
