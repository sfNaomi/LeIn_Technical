<aura:component implements="force:lightningQuickActionWithoutHeader,force:hasRecordId,force:hasSObjectName">
    <div class="modal-margin">
		<c:resetBenchmark
			recordId="{! v.recordId }"
			objectApiName="{! v.sObjectName }"
			isAura="true"
			oncloseaction="{! c.closeAction }"
		></c:resetBenchmark>
	</div>
</aura:component>